{
  "name": "universal-react-redux",
  "version": "1.0.0",
  "description": "universal react redux boilerplate",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:johnnyghost/universal-react-redux.git",
    "type": "git"
  },
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist",
    "lint": "./node_modules/.bin/eslint --config .eslintrc src",
    "pre-build": "npm run clean && npm run lint",
    "babel-node": "babel-node --presets es2015,stage-0 --ignore node_modules,dist",
    "test": "cross-env NODE_ENV=test karma start configs/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "start": "NODE_ENV=prod npm run build:production && NODE_ENV=prod npm run babel-node -- server/index.js",
    "watch:client": "npm run clean && ./node_modules/.bin/webpack-dev-server --config ./configs/webpack/./webpack.development.client.babel.js",
    "watch:server": "npm run clean && NODE_ENV=dev ./node_modules/.bin/nodemon",
    "build:client": "webpack --colors --config ./configs/webpack/client/webpack.config",
    "build:server": "webpack --colors --config ./configs/webpack/server/webpack.config",
    "build:development": "NODE_ENV=dev npm run build:client && NODE_ENV=dev npm run build:server",
    "build:production": "npm run pre-build && NODE_ENV=prod npm run build:client && NODE_ENV=prod npm run build:server",
    "build:staging": "npm run pre-build && NODE_ENV=staging npm run build:client && NODE_ENV=staging npm run build:server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "redux",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "boilerplate",
    "babel"
  ],
  "author": "johnnyghost <kemarx@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "1.6.2",
    "express": "4.14.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-flexbox-grid": "0.10.2",
    "react-helmet": "4.0.0",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "redux-saga": "0.14.3",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "2.35.3",
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "4.0.0",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-plugin-typecheck": "3.9.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.22.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "chunk-manifest-webpack-plugin": "1.0.0",
    "classnames": "2.2.5",
    "cross-env": "^3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "flexboxgrid": "^6.3.1",
    "html-loader": "0.4.4",
    "image-webpack-loader": "3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "lodash": "^4.17.4",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "null-loader": "0.1.1",
    "postcss-easy-import": "2.0.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.0",
    "precss": "1.4.0",
    "progress-bar-webpack-plugin": "1.9.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-manifest-plugin": "1.1.0"
  }
}
